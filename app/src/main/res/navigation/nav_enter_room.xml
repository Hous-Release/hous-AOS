<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_enter_room"
    app:startDestination="@id/enterRoomFragment">

    <fragment
        android:id="@+id/enterRoomFragment"
        android:name="hous.release.android.presentation.enter_room.EnterRoomFragment"
        android:label="EnterRoomFragment" >
        <action
            android:id="@+id/action_enterRoomFragment_to_createRoomFragment"
            app:destination="@id/createRoomFragment" />
        <action
            android:id="@+id/action_enterRoomFragment_to_enterRoomCodeFragment"
            app:destination="@id/enterRoomCodeFragment" />
    </fragment>
    <fragment
        android:id="@+id/createRoomFragment"
        android:name="hous.release.android.presentation.enter_room.CreateRoomFragment"
        android:label="CreateRoomFragment" />
    <fragment
        android:id="@+id/enterRoomCodeFragment"
        android:name="hous.release.android.presentation.enter_room.EnterRoomCodeFragment"
        android:label="EnterRoomCodeFragment" />
</navigation>
