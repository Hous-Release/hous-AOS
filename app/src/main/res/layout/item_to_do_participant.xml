<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="8dp"
    android:background="@drawable/shape_g_1_fill_6_rect">

    <ImageView
        android:id="@+id/iv_to_do_personal_color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="7dp"
        android:layout_marginStart="6dp"
        android:src="@drawable/shape_blue_fill_12_circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_to_do_participant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="7dp"
        android:textColor="@color/hous_g_6"
        android:theme="@style/Description"
        app:layout_constraintBottom_toBottomOf="@+id/iv_to_do_personal_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_to_do_personal_color"
        app:layout_constraintTop_toTopOf="@+id/iv_to_do_personal_color"
        tools:text="강원용" />

</androidx.constraintlayout.widget.ConstraintLayout>